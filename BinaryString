#include <iostream>
#include <string>
using namespace std;

int OperationsBinaryString(const string &str) {
    if (str.empty())
        return -1;

    int result = str[0] - '0';

    for (size_t i = 1; i < str.size(); i += 2) {
        char op = str[i];
        int num = str[i + 1] - '0';

        if (op == 'A')
            result &= num;
        else if (op == 'B')
            result |= num;
        else if (op == 'C')
            result ^= num;
    }

    return result;
}

int main() {
    string s;
    getline(cin, s);
    cout << OperationsBinaryString(s);
}
